
@{
    ViewBag.Title = "EventDetail";
}

<h2>@ViewBag.e.EventName</h2>
<div class="MainBody">
    <h5>@ViewBag.e.StreamGame</h5>
    <h5>Description: @ViewBag.e.Description</h5>
    <h5>Start: @ViewBag.e.StartDate</h5>
    <h5>End: @ViewBag.e.EndDate</h5>
    <h5>End: @ViewBag.e.StreamGame</h5>

    @{
        if (Session["IsLoggedIn"].ToString() == "true")
        {
            <a href="~/Home/EventEdit">Edit Event</a>
        }
    }



</div>
